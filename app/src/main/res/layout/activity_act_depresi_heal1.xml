<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/backgroundgrey"
    >

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="100dp"
            >
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="140dp"
                android:background="@drawable/anx1"
                />
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="140dp"
                android:background="@drawable/anx2"
                android:layout_marginTop="140dp"
                />
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="140dp"
                android:background="@drawable/anx3"
                android:layout_marginTop="280dp"
                />
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="140dp"
                android:background="@drawable/anx4"
                android:layout_marginTop="420dp"
                />
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="140dp"
                android:background="@drawable/anx5"
                android:layout_marginTop="560dp"
                />
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="140dp"
                android:background="@drawable/anx6"
                android:layout_marginTop="700dp"
                />
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="140dp"
                android:background="@drawable/anx7"
                android:layout_marginTop="840dp"
                />
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="110dp"
                android:background="@drawable/anx8"
                android:layout_marginTop="980dp"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="1150dp"
                android:text="Ingin konsul dengan psikolog? sila klik tombol di bawah"
                android:textColor="#C062F5"
                android:textSize="12dp" />
            <Button
                android:id="@+id/callKlinikD"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="1180dp"
                android:text="klinik psikologi yarsi"
                android:layout_marginRight="20dp"
                android:layout_marginLeft="20dp"
                android:background="@drawable/custombutton"
                android:textSize="15dp"
                android:textColor="#ffff"
                />

            <Button
                android:id="@+id/backTestD"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="1250dp"
                android:layout_marginRight="20dp"
                android:layout_marginBottom="50dp"
                android:background="#ffff"
                android:text="lanjut Mengukur Depresi"
                android:textColor="#C062F5"
                android:textSize="15dp" />
        </RelativeLayout>

    </ScrollView>

</RelativeLayout>